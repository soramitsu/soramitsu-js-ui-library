{
  "name": "@soramitsu-ui/ui",
  "version": "0.1.0",
  "main": "dist/lib.cjs.js",
  "module": "dist/lib.es.js",
  "types": "dist/lib.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/lib.es.js",
      "require": "./dist/lib.cjs.js"
    },
    "./styles": "./dist/style.css",
    "./*": "./*"
  },
  "scripts": {
    "sb:build": "build-storybook",
    "sb:serve": "start-storybook -p 6006",
    "cy": "cypress open-ct",
    "cy:ci": "cypress run-ct --quiet --reporter spec",
    "typecheck": "vue-tsc --noEmit",
    "gulpts": "gulp --require sucrase/register -f gulpfile.ts",
    "clean": "yarn gulpts clean",
    "build": "yarn gulpts",
    "build:tsc": "tsc --outDir dist-ts --emitDeclarationOnly --declaration",
    "build:vite": "vite build",
    "api:extract": "api-extractor run",
    "api:extract:local": "api-extractor run --local"
  },
  "dependencies": {
    "@popperjs/core": "^2.10.2",
    "@vueuse/core": "^6.7.4",
    "body-scroll-lock": "^4.0.0-beta.0",
    "focus-trap": "^6.7.1",
    "jsoneditor": "^9.5.5",
    "lodash": "^4.17.21",
    "vue": "3.2.12"
  },
  "devDependencies": {
    "@cypress/vite-dev-server": "^2.2.0",
    "@cypress/vue": "^3.0.3",
    "@iconify/json": "^1.1.426",
    "@jest/types": "^27.2.4",
    "@mdx-js/preact": "^1.6.22",
    "@microsoft/api-extractor": "^7.18.19",
    "@soramitsu-ui/icons": "^0.1.0",
    "@soramitsu-ui/theme": "^0.1.0",
    "@storybook/addon-actions": "^6.3.9",
    "@storybook/addon-essentials": "^6.3.9",
    "@storybook/addon-links": "^6.3.9",
    "@storybook/vue3": "^6.3.9",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/gulp": "^4.0.9",
    "@types/jsoneditor": "^8.6.1",
    "@types/lodash": "^4.14.173",
    "@vitejs/plugin-vue": "^1.8.1",
    "@vitejs/plugin-vue-jsx": "^1.2.0",
    "@vue/compiler-sfc": "3.2.12",
    "cypress": "^8.4.1",
    "cypress-plugin-snapshots": "^1.4.4",
    "del": "^6.0.0",
    "execa": "^5.1.1",
    "fast-glob": "^3.2.7",
    "gulp": "^4.0.2",
    "sass": "^1.41.1",
    "storybook-builder-vite": "^0.0.12",
    "sucrase": "^3.20.3",
    "type-fest": "^2.3.4",
    "typescript": "^4.4.4",
    "unplugin-icons": "^0.12.18",
    "vite": "^2.5.10",
    "vite-plugin-windicss": "^1.4.3",
    "vite-svg-loader": "^3.1.0",
    "vue-dts-gen": "^0.3.0",
    "vue-tsc": "^0.3.0",
    "windicss": "^3.1.7",
    "zx": "^4.2.0"
  }
}
