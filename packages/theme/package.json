  },
  "dependencies": {
    "windicss": "^3.1.7"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.4",
    "del": "^6.0.0",
    "del-cli": "^4.0.1",
    "rollup": "^2.56.3",
    "rollup-plugin-dts": "^4.0.0",
    "rollup-plugin-esbuild": "^4.5.0",
    "sucrase": "^3.20.1"
  },
  "publishConfig": {
    "main": "dist/lib.cjs.js",
    "module": "dist/lib.esm.js",
    "types": "dist/lib.d.ts"
  }
}
